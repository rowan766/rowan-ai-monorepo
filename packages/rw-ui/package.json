{
  "name": "@rowan/rw-ui",
  "version": "1.0.1",
  "description": "React UI component library for Rowan AI",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "typescript"
  ],
  "author": "ro wan",
  "license": "ISC",
  "dependencies": {
    "@rowan/rw-hooks": "workspace:*",
    "@rowan/rw-libs": "workspace:*",
    "clsx": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.60.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  }
}
