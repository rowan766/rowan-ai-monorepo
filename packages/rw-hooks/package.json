{
  "name": "@rowan/rw-hooks",
  "version": "1.0.1",
  "description": "React hooks library for Rowan AI",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "author": "ro wan",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@rowan/rw-libs": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^3.29.4",
    "typescript": "^5.0.0"
  }
}